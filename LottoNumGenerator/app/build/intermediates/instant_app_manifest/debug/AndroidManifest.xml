<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lottonumgenerator"
    android:targetSandboxVersion="2"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="30" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:icon="@mipmap/kr_lotto"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/kr_lotto"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/AppTheme" >

        <!-- AppTheme.Result 테마지정 -->
        <activity
            android:name="com.example.lottonumgenerator.ResultActivity"
            android:theme="@style/AppTheme.Result" />
        <!-- AppTheme.Name 테마지정 -->
        <activity
            android:name="com.example.lottonumgenerator.NameActivity"
            android:theme="@style/AppTheme.Name" />
        <!-- AppTheme.Constellation 테마지정 -->
        <activity
            android:name="com.example.lottonumgenerator.ConstellationActivity"
            android:theme="@style/AppTheme.Constellation" />
        <activity android:name="com.example.lottonumgenerator.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>